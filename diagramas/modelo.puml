@startuml modelo
class Produto{
	codigo
	data_cadastro
}

class ItemEntrada{
	quantidade
	custo_unitario
}

class NotaEntrada{
	numero
	loja
	data
	hora
}

class ItemSaida{
	quantidade
	custo_unitario
}

class NotaSaida{
	loja
	data
	hora
}

class Saldo{
	loja
	quantidade
}

NotaEntrada	<-- ItemEntrada
NotaSaida	<-- ItemSaida
ItemEntrada	-->	Produto
ItemSaida	-->	Produto
Saldo		-u->	Produto
@enduml
